{
  "schemaVersion": 1,
  "id": "lootcrates",
  "version": "${version}",
  "name": "Modular Loot Crates",
  "description": "Lockable highly dynamic loot chests and bags for fabric",
  "authors": [
    "DaFuqs"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/fabric-loot-crates",
    "sources": "github.com/DaFuqs/LootCrates",
    "issues": "github.com/DaFuqs/LootCrates/issues"
  },
  "license": "MIT",
  "icon": "assets/lootcrates/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "de.dafuqs.lootcrates.LootCratesClient"
    ],
    "main": [
      "de.dafuqs.lootcrates.LootCrates"
    ],
    "modmenu": [
      "de.dafuqs.lootcrates.config.ModMenuConfig"
    ],
    "quickshulker": [
      "de.dafuqs.lootcrates.compat.QuickShulkerCompat"
    ]
  },
  "mixins": [
    "lootcrates.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": ">=1.17.0",
    "fabric": ">=0.36.0",
    "cloth-config2": ">=5.0.34"
  },
  "suggests": {
    "quickshulker": "1.2.2-1.17"
  }
}
